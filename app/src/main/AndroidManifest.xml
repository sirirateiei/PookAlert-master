<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rtc.kanchana.sirirat.pookalert">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetupTime" />
        <activity android:name=".SetupSound" />
        <activity android:name=".ConfirmTime" />
        <activity android:name=".Notification"></activity>

        <receiver android:name=".MyReceive" android:process=":remote"/>

    </application>

</manifest>